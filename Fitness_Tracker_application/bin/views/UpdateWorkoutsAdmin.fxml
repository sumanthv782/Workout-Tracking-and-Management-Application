<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="360.0" prefWidth="560.0" style="-fx-background-color: bbbdbd;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UpdateWorkoutsAdminController">
   <children>
      <TableView fx:id="updateWorkoutsTable" editable="false" layoutX="0" layoutY="0" prefHeight="300.0" prefWidth="560.0">
         <columns>
            <!-- Column for Exercise -->
            <TableColumn fx:id="colMuscleGroup" prefWidth="110.0" text="Muscle Group" />
            
            <TableColumn fx:id="colExercise" prefWidth="140.0" text="Exercise" />

            <!-- Column for Difficulty -->
            <TableColumn fx:id="colDifficulty" prefWidth="100.0" text="Difficulty" />

            <!-- Column for Reps -->
            <TableColumn fx:id="colReps" prefWidth="100.0" text="Reps_Required" />
            
         </columns>
      </TableView>
      
	  <Button layoutX="80.0" layoutY="325.0" mnemonicParsing="false" text="Update" onAction="#handleUpdateExerciseButtonClicked" />
	  <Button layoutX="140.0" layoutY="325.0" mnemonicParsing="false" text="Delete" onAction="#handleDeleteExerciseButtonClicked"/>
   </children>
</AnchorPane>
